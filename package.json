{
  "name": "create-react-component",
  "version": "0.1.0",
  "repository": "https://github.com/einsiol/react-create-component",
  "description": "Automatically create a component folder structure as well as adding it to the main index file",
  "main": "dist/index.js",
  "scripts": {
    "prestart": "mkdir -p temp",
    "start": "babel src -d dist && node dist/index.js",
    "build": "rm -rf dist && babel src -d dist",
    "test": "ava",
    "lint": "eslint src --cache",
    "docs": "jsdoc -d ./docs -t node_modules/docdash -r ./src && live-server docs"
  },
  "author": "Einar Ã“lafsson",
  "license": "GPL-3.0",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-modules-commonjs": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/register": "^7.9.0",
    "ava": "3.8.2",
    "babel-eslint": "^10.1.0",
    "babel-plugin-minify-mangle-names": "^0.5.0",
    "babel-preset-minify": "^0.5.1",
    "eslint": "^7.0.0",
    "eslint-plugin-ava": "^10.3.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "esm": "^3.2.25",
    "husky": "^4.2.5"
  },
  "dependencies": {
    "arg": "^4.1.3",
    "command-line-args": "^5.1.1"
  }
}
